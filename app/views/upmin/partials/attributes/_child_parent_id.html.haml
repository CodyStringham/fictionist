.form-group{class: attribute.errors? ? "has-error" : ""}
  %label{for: attribute.form_id}
    = 'Parent'
  %h1 hey
  - if attribute.editable? && f = form_builder
    .enum
      = f.collection_select :parent_id, Parent.order(:name), :id, :name, include_blank: true

  - else
    %p.well
      = attribute.value
